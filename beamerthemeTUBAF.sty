			\frame[plain]{\usebeamertemplate{title page}\@thanks}
			%\setbeamercolor{background canvas}{use=normal text,bg=,fg=normal text.fg}
			\setbeamercolor{background canvas}{use=normal text,bg=,fg=normal text.fg}
