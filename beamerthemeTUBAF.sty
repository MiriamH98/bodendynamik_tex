			\frame[plain]{\usebeamertemplate{title page}\@thanks}
			\setbeamercolor{background canvas}{use=normal text,bg=normal text.bg,fg=normal text.fg}
		\fi
	}{%
	\ifbeamer@inframe
		\usebeamertemplate{title page}\@thanks
	   \else
		\frame[plain]{\usebeamertemplate{title page}\@thanks}
	\fi
	}}%
}
