	\ifthenelse{\equal{#1}{4}}{\renewcommand{\TUBAFhelligkeitgrau}{#1}\renewcommand{\TUBAFgrauext}{iv}}{%
	\ifthenelse{\equal{#1}{5}}{\renewcommand{\TUBAFhelligkeitgrau}{#1}\renewcommand{\TUBAFgrauext}{v}}{%
	\ifthenelse{\equal{#1}{6}}{\renewcommand{\TUBAFhelligkeitgrau}{#1}\renewcommand{\TUBAFgrauext}{vi}}{%
	\ifthenelse{\equal{#1}{7}}{\renewcommand{\TUBAFhelligkeitgrau}{#1}\renewcommand{\TUBAFgrauext}{vii}}{%
	\ifthenelse{\equal{#1}{8}}{\renewcommand{\TUBAFhelligkeitgrau}{#1}\renewcommand{\TUBAFgrauext}{viii}}{%
