% die Lektionsseite
\AtBeginLecture{%
	\mode<presentation>{%
		\ifthenelse{\equal{\TUBAF@stil}{lehre}}{
			\frame[plain]{%
				\frametitle{{\usebeamerfont{framesubtitle}\insertshortTUBAFAutor:~\insertshortTUBAFTitel~\\[2ex]} \TUBAFlektionstitel}
				%\frametitle{\TUBAFlektionstitel}
				\vskip0pt plus 1filll
				\begin{center}\Large\bfseries\insertlecture\end{center}
				\vskip0pt plus 1filll\vskip3cm
			}%
		   }{%
			\frame{%
				\frametitle{\TUBAFlektionstitel}
				\vskip0pt plus 1filll
				\begin{center}\Large\bfseries\insertlecture\end{center}
				\vskip0pt plus 1filll\vskip3cm
			}%
		}
			\addtocounter{framenumber}{-1}%
	}
	\mode<article>{%
		\vskip1.5em
		\noindent{\ifthenelse{\isundefined{\usekomafont}}{}{\usekomafont{sectioning}}%
		\Large\bfseries\TUBAFlektionstitel
		\newline
		\huge\insertlecture
		\vskip1em}
	}
}
